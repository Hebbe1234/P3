@{
    List<NorthernWinterBeatLibrary.Models.Participant> participants = new List<NorthernWinterBeatLibrary.Models.Participant>(Model.Participants);

    participants = Model.SortParticipants(participants); 

    int i = 0;
    foreach (var item in participants)
    {
    <tr onclick="location.href = '@(Url.Page("ParticipantPage", new { id = item.ID }))'">
        <td>
            @item.Email
        </td>
        <td>
            @item.Ticket?.TicketNumber
        </td>
        <td>
            @item.Name
        </td>
        @*<td>
            @item.State
        </td>*@
    </tr>
        i++;
    }
}
